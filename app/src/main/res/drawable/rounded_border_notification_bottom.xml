        <corners android:bottomLeftRadius="@dimen/_25sdp" android:bottomRightRadius="@dimen/_25sdp"></corners>
        <stroke android:width="1dp" android:color="#fff" />
        <solid android:color="#FFFFFF" />
    </shape>
</item>

</layer-list>