<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">
<LinearLayout
    android:id="@+id/llFilter"
    android:elevation="@dimen/_10sdp"
    android:padding="@dimen/_8sdp"
    android:background="@drawable/rounded_border_up_side_white"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_margin="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.driveway.Component.TTextView
            android:id="@+id/ParkingTitle"
            android:paddingRight="@dimen/_6sdp"
            android:text=""
            android:layout_toLeftOf="@id/tvKM"
            android:textStyle="bold"
            android:textColor="#EC6004"
            android:textSize="@dimen/_16ssp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.driveway.Component.TTextView>
        <com.driveway.Component.TTextView
            android:id="@+id/tvKM"
            android:text=""
            android:singleLine="true"
            android:ellipsize="end"
            android:gravity="left|center"
            android:layout_alignParentRight="true"
            android:textStyle="bold"
            android:textColor="#EC6004"
            android:textSize="@dimen/_16ssp"
            android:layout_width="@dimen/_80sdp"
            android:layout_height="wrap_content"></com.driveway.Component.TTextView>
    </RelativeLayout>
    <RelativeLayout
        android:layout_margin="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.driveway.Component.TTextView
            android:textSize="@dimen/_13ssp"
            android:paddingRight="@dimen/_10sdp"
            android:layout_width="match_parent"
            android:drawableLeft="@drawable/drawable_maps_black"
            android:layout_height="wrap_content"
            android:gravity="center|left"
            android:text=""
            android:layout_toLeftOf="@id/tvMinutes"
            android:drawablePadding="@dimen/_3sdp"
            android:id="@+id/PropertyAddress"
            android:textColor="#575757"
            android:textStyle="normal"></com.driveway.Component.TTextView>
        <com.driveway.Component.TTextView
            android:drawableLeft="@drawable/drawable_left_watch"
            android:textSize="@dimen/_13ssp"
            android:gravity="center|left"
            android:drawablePadding="@dimen/_5sdp"
            android:layout_alignParentRight="true"
            android:textColor="#575757"
            android:id="@+id/tvMinutes"
            android:text=""
            android:ellipsize="end"
            android:singleLine="true"
            android:layout_width="@dimen/_80sdp"
            android:layout_height="wrap_content"></com.driveway.Component.TTextView>
    </RelativeLayout>
    <RelativeLayout
        android:layout_margin="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.driveway.Component.TTextView
            android:textSize="@dimen/_12ssp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|left"
            android:text=""
            android:paddingLeft="@dimen/_13sdp"
            android:layout_toLeftOf="@id/tvRatings"
            android:drawablePadding="@dimen/_3sdp"
            android:id="@+id/tvAvailability"
            android:textColor="#575757"
            android:textStyle="normal"></com.driveway.Component.TTextView>
        <com.driveway.Component.TTextView
            android:textSize="@dimen/_12ssp"
            android:gravity="center|left"
            android:singleLine="true"
            android:ellipsize="end"
            android:layout_alignParentRight="true"
            android:textColor="#333232"
            android:drawablePadding="@dimen/_5sdp"
            android:id="@+id/tvRatings"
            android:drawableLeft="@drawable/drawable_star_black"
            android:text=""
            android:layout_width="@dimen/_80sdp"
            android:layout_height="wrap_content"></com.driveway.Component.TTextView>
    </RelativeLayout>
    <View
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginBottom="@dimen/_4sdp"
        android:background="#B2B2B2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"></View>
    <LinearLayout
        android:layout_margin="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:gravity="center"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent">
            <com.driveway.Component.TTextView
                android:textSize="@dimen/_10ssp"
                android:layout_width="match_parent"
                android:drawableLeft="@drawable/drawable_car_black"
                android:layout_height="wrap_content"
                android:gravity="center|left"
                android:text="Parking Type"
                android:drawablePadding="@dimen/_3sdp"
                android:id="@+id/tvParking"
                android:textColor="#8B8B8B"
                android:textStyle="normal"></com.driveway.Component.TTextView>

            <com.driveway.Component.TTextView
                android:id="@+id/tvType"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="wrap_content"
                android:layout_below="@id/tvParking"
                android:layout_centerVertical="true"
                android:layout_marginTop="@dimen/_6sdp"
                android:background="@drawable/border_button_orange_with_large_curve"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:text="Driveway"
                android:textColor="#4E4B4B"
                android:textSize="@dimen/_11ssp"></com.driveway.Component.TTextView>
        </RelativeLayout>
        <LinearLayout
            android:layout_weight="2"
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content">

            <com.driveway.Component.RoundCornersImageView
                android:layout_weight="1"
                android:id="@+id/imgone"
                android:layout_width="0dp"
                android:scaleType="fitXY"
                android:layout_height="@dimen/_70sdp"></com.driveway.Component.RoundCornersImageView>

            <com.driveway.Component.RoundCornersImageView
                android:layout_weight="1"
                android:id="@+id/imgtwo"
                android:layout_marginLeft="@dimen/_6sdp"
                android:layout_toRightOf="@id/imgone"
                android:layout_width="0dp"
                android:scaleType="fitXY"
                android:layout_height="@dimen/_70sdp"></com.driveway.Component.RoundCornersImageView>

        </LinearLayout>

    </LinearLayout>
    <View
        android:layout_marginTop="@dimen/_4sdp"
        android:layout_marginBottom="@dimen/_4sdp"
        android:background="#B2B2B2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_1sdp"></View>

    <RelativeLayout
        android:layout_margin="@dimen/_5sdp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.driveway.Component.TTextView
            android:id="@+id/tvPrice"
            android:text=""
            android:gravity="center|left"
            android:layout_centerInParent="true"
            android:layout_toLeftOf="@id/bookNow"
            android:textStyle="bold"
            android:textColor="#EC6004"
            android:textSize="@dimen/_18ssp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"></com.driveway.Component.TTextView>
        <com.driveway.Component.TTextView
            android:text="BOOK NOW"
            android:id="@+id/bookNow"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:textStyle="bold"
            android:textColor="@color/white"
            android:background="@drawable/booking_button_round"
            android:textSize="@dimen/_10ssp"
            android:layout_width="@dimen/_120sdp"
            android:layout_height="@dimen/_30sdp"></com.driveway.Component.TTextView>
    </RelativeLayout>
</LinearLayout>
</RelativeLayout>